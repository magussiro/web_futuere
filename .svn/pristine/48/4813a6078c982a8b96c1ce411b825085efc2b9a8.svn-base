.price_amount_header:after ,.amount_price:after { clear: both; display: block; content: ""; }

.price_amount_header li ,.amount_price li { float: left; height: 22px; display: block !important; }
.price_amount_header li.one ,.amount_price li.one { width: 30% !important; }
.price_amount_header li.two ,.amount_price li.two { width: 20% !important; }
.price_amount_header li.three,.price_amount_header li.four ,
.amount_price li.three,.amount_price li.four { width: 25% !important; }
.amount_price_open ,.amount_price_now ,.amount_price_high ,.amount_price_low{
    border-width: 1px;
    border-style: solid;
}

.amount_price_open { border-color:#1c3e2e; border-bottom-color: #1c3e2e !important;  }
.amount_price_now { border-color:#82d8cf; border-bottom-color:#82d8cf !important; }
.amount_price_high { border-color:#f00; border-bottom-color:#f00 !important; }
.amount_price_low { border-color:#01b72c; border-bottom-color:#01b72c !important; }

.amount_price li.two { position: relative; height: 22px; }

.amount_price_now li.two:before ,
.amount_price_high li.two:after ,
.amount_price_low li.two:after {
    display: block; position: absolute; top: 0; left: 0; width: 100%; text-align: center;
}

.amount_price_now li.two:before { content: "現價"; }
.amount_price_high li.two:after { content: "最高"; }
.amount_price_low li.two:after { content: "最低"; }

/*開+現*/
.amount_price_open.amount_price_now{
    box-shadow: 0px 0px 0px 2px #82d8cf inset
}
/*開+現+高*/
.amount_price_open.amount_price_now.amount_price_high{
    box-shadow: 0px 0px 0px 2px #82d8cf inset ,0px 0px 0px 4px #f00 inset;
}
/*開+現+低*/
.amount_price_open.amount_price_now.amount_price_low{
    box-shadow: 0px 0px 0px 2px #82d8cf inset ,0px 0px 0px 4px #01b72c inset;
}
/*開+高*/
.amount_price_open.amount_price_high{
    box-shadow: 0px 0px 0px 2px #f00 inset
}
/*開+低*/
.amount_price_open.amount_price_low{
    box-shadow: 0px 0px 0px 2px #01b72c inset
}

/*現+高*/
.amount_price_now.amount_price_high{
    box-shadow: 0px 0px 0px 2px #f00 inset
}
/*現+低*/
.amount_price_now.amount_price_low{
    box-shadow: 0px 0px 0px 2px #01b72c inset
}

/*老師人物設定*/
#divTeacherList { background-color:white; color:black; padding:10px; height:100%; overflow:hidden; }
#divTeacherList h2 { font-weight:bold; font-size:20px; }
#divTeacherList .teacher_list { width:100%; margin:15px 0; height:80%; overflow:auto; font-size:0; }
#divTeacherList label { width:20%; margin:5px 0; font-size:16px; display:inline-block; }
#divTeacherList input[type=checkbox] { margin-right:3px; }
.hidden { display:none; }

#onlinestatus.online { color: #0f0; }
#onlinestatus.delay { color: #fa0; }
#onlinestatus.offline { color: #f00; }


/*預設額度數字顏色*/
#left_cont .user_money{
	color: rgb(11,17,155);
}
#left_cont .user_money.minus{
	color: rgb(235,51,35);
}

/*新增勾選平倉樣式*/
.close_msg.notice{
	color: rgb(235,51,35);
}
/*商品統計 - 總多數字紅色/ 總空數字綠色*/
#list_static td:nth-child(2){
	color: rgb(235, 97, 107);
}
#list_static td:nth-child(3){
	color: rgb(1, 183, 44);
}