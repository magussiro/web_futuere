/* header */

header {
    clear: both;
    /*background: rgba(229, 248, 255, 0.2);*/
    width: 99%;
    margin: auto;
    /*margin-top: 10px;
    margin-bottom: 10px;*/
    margin-top: 5px;
    margin-bottom: 5px;
    border-radius: 5px;
    padding: 10px;
    /*padding: 1px 0px 6px 5px;*/
    /* padding: 1px 0px 1px 5px; */
    color: #000;
    font-size: 14px;
    background: url('images/nav0001.png') 0 0 no-repeat;
    background-size: cover;
}

header h1 {
    display: inline-block;
    vertical-align: middle;
    font-size: 20px;
}

header #logo {
    height: 30px;
    margin: 5px 0px 5px 10px;
    display: inline-block;
    vertical-align: middle;
}

header marquee {
    font-size: 15px;
    position: relative;
    top: 4px;
    color: #66d9ff;
}

header div {
    display: inline-block;
    vertical-align: middle;
    /*width: 22%;*/
    width: 26%;
    text-align: right;
    float: right;
    /*margin: 9px 10px 0px 0px;*/
    margin: 0px 10px 0px 0px;
}

header button {
    font-size: 12px;
    color: #fff;
    background: rgba(173, 196, 197, 1.00);
    border: 2px solid rgba(89, 115, 117, 1.00);
    border-radius: 5px;
    cursor: pointer;
    padding: 0px 6px;
    height: 22px;
}

header button:hover {
    border: 2px solid #fff;
}

