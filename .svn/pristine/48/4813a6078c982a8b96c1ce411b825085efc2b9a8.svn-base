/* all */
/* 原色 : #eb6b61; */
@font-face {
    font-family: 'Roboto Mono';
    src: url(../font/RobotoMono-Medium.ttf);
}

@font-face {
    font-family: 'Noto Sans TC';
    src: url(../font/NotoSansTC-Light.otf);
}

*,
*:before,
*:after {
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
}

html,
body {
    min-width: 1429px;
    /* min-width: 1260px; */
    width: 100%;
    height: 100%;
    overflow-x: hidden;
}

body {
    background-image: url("");
    background-color: #ddd;
    background-position: center;
    /*background-repeat: no-repeat;*/
    background-size: cover;
    font-size: 0px;
}

body:before {
    content: '';
    display: inline-block;
    vertical-align: middle;
    width: 0px;
    height: 100%;
}

body * {
    font-family: 'Roboto Mono', 'Noto Sans TC', sans-serif;
}

#wrap {
    /*min-width: 1310px;*/
    /* min-width: 1260px; */
    width: 100%;
    vertical-align: middle;
    display: inline-block;
    box-shadow:3px 3px 16px #000;
    padding: 30px 0;
}

.upColor {
    color: rgba(175, 116, 44, 1.00) !important;
}

.downColor {
    color: #55DF56 !important;
}


/* #company_logo */

#wrap #company_logo {
    margin: auto;
    width: 200px;
    height: 131px;
    background-image: url("../img/company_logo1.png");
    background-size: cover;
    margin-bottom: 20px;
    opacity: 1;
    color: #fff;
    font-size: 24px;
    text-align: center;
    padding-top: 93px;
    padding-left: 18px;
    letter-spacing: 15px;
}


/* #header */

#wrap #header {
    /*padding: 5px;*/
    padding: 3px;
    /* background: rgba(229, 248, 255, 0.2); */
    background: url('images/nav0001.png') 0 0 no-repeat;
    background-size: cover;
    width: 99%;
    margin: auto;
    border-radius: 5px;
    font-size: 0px;
}

#wrap #header .header_left,
#wrap #header .header_middle,
#wrap #header .header_right {
    display: inline-block;
    vertical-align: middle;
}

#wrap #header .header_left ul.mainmenu li.main,
#wrap #header .header_middle ul.mainmenu li.main,
#wrap #header .header_right ul.mainmenu li.main {
    display: inline-block;
    vertical-align: top;
    padding: 5px;
}

#wrap #header .header_left {
    width: 20%;
    font-size: 0px;
}

#wrap #header .header_left ul.mainmenu,
#wrap #header .header_right ul.mainmenu {
    position: relative;
}

#wrap #header .header_left ul.mainmenu li.main,
#wrap #header .header_right ul.mainmenu li.main {
    padding: 5px 10px;
    display: inline-block;
    vertical-align: middle;
    cursor: pointer;
    color: #fff;
    width: 23%;
    font-size: 16px;
}

#wrap #header .header_left ul.mainmenu li.main:hover,
#wrap #header .header_right ul.mainmenu li.main:hover {
    color: rgba(175, 116, 44, 1.00);
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu,
#wrap #header .header_right ul.mainmenu li.main ul.submenu {
    position: absolute;
    border: 2px solid #111;
    border-radius: 5px;
    background: rgba(0, 0, 0, 0.8);
    margin-left: -10px;
    margin-top: 5px;
    display: none;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub,
#wrap #header .header_right ul.mainmenu li.main ul.submenu li.sub {
    display: block;
    line-height: 2;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    padding: 5px;
    text-align: left;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub a,
#wrap #header .header_right ul.mainmenu li.main ul.submenu li.sub a {
    display: block;
    font-size: 13px;
    color: #fff;
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub a:hover,
#wrap #header .header_right ul.mainmenu li.main ul.submenu li.sub a:hover {
    color: rgba(173, 196, 197, 1.00);
}

#wrap #header .header_left ul.mainmenu li.main ul.submenu li.sub:last-child {
    border-bottom: none;
}

#wrap #header .header_middle {
    width: 69%;
    color: #eee;
    font-size: 16px;
}

#wrap #header .header_right i {
    font-size: 20px;
    color: #fff;
}

#wrap #header .header_right i:hover {
    color: rgba(175, 116, 44, 1.00);
}

#wrap #header .header_right {
    width: 10%;
    text-align: right;
    font-size: 16px;
}

#wrap #header .header_right ul.mainmenu li.main {
    cursor: pointer;
}


